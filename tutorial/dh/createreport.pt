<!DOCTYPE html>
<html lang="en">

<head>
    <title>Tạo báo biểu</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" integrity="sha384-HSMxcRTRxnN+Bdg0JdbxYKrThecOKuH5zCYotlSAcp1+c8xmyTe9GYg1l9a69psu" crossorigin="anonymous">
</head>

<body>
    <div class="container">
        <section id="inner" class="inner-section section">
            <div class="container">

                <!-- SECTION HEADING -->
                <h2 class="section-heading text-center wow fadeIn" data-wow-duration="1s">Tạo báo biểu</h2>
                <div class="row">
                    <div class="col-md-6 col-md-offset-3 text-center">
                        <a class="wow fadeIn" href="/">Home</a>
                    </div>
                </div>
                <div class="inner-wrapper row">
                    <div class="col-md-12">
                        <form method="POST" action="${request.route_url('createBBHDBH')}" class="col-md-6 col-md-offset-3">
                            <div class="form-group">
                                <label for="from">Từ</label>
                                <input class="form-control" type="date" name="from"/>
                            </div>
                            <div class="form-group">
                                <label for="to">Đến</label>
                                <input class="form-control" type="date" name="to"/>
                            </div> 
                            <div class="form-group">
                                <button type="submit" style="width: 100%" class="btn btn-primary" name="form.export">Tạo báo biểu bán hàng</button>
                                
                            </div>
                        </form>
                        <form method="POST" action="${request.route_url('createBBHDNH')}" class="col-md-6 col-md-offset-3">
                            <div class="form-group">
                                <label for="from">Từ</label>
                                <input class="form-control" type="date" name="from"/>
                            </div>
                            <div class="form-group">
                                <label for="to">Đến</label>
                                <input class="form-control" type="date" name="to"/>
                            </div> 
<!--                             <div class="form-group">
                                <button type="submit" style="width: 100%" class="btn btn-primary" name="form.export">Tạo báo biểu bán hàng</button>
                                
                            </div> -->
                            <div class="form-group">
                                <button type="submit" style="width: 100%" class="btn btn-primary" name="form.import">Tạo báo biểu nhập hàng</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </section>
    </div>
    <script src="https://code.jquery.com/jquery-1.12.4.min.js" integrity="sha384-nvAa0+6Qg9clwYCGGPpDQLVpLNn0fRaROjHqs13t4Ggj3Ez50XnGQqc/r8MhnRDZ" crossorigin="anonymous"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js" integrity="sha384-aJ21OjlMXNL5UyIl/XNwTMqvzeRMZH2w8c5cRVpzpU8Y5bApTppSuUkhZXN0VxHd" crossorigin="anonymous"></script>
</body>